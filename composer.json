{
    "name": "commune/platform-wechat",
    "description": "wechat platform adapter for chatbot framework commune/studio-hyperf",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "thirdgerb",
            "email": "thirdgerb@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Commune\\Platform\\Wechat\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3",
        "commune/chatbot-hyperf": "dev-master",
        "overtrue/wechat": "~4.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "swoft/swoole-ide-helper": "^4.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../chatbot-hyperf"
        },
        {
            "type": "path",
            "url": "../chatbot"
        }
    ]
}
